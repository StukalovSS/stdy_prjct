{
  "name": "snake-web",
  "config": {
    "dev": "--config ./dev.config.mjs",
    "prod": "--config ./prod.config.mjs"
  },
  "scripts": {
    "start-dev": "webpack serve ${npm_package_config_dev}",
    "start-prod": "webpack serve ${npm_package_config_prod}",
    "build-dev": "webpack ${npm_package_config_dev}",
    "build-prod": "webpack ${npm_package_config_prod} --stats-children",
    "clean": "rd /s /q build",
    "lint": "eslint src --ext .js --ext .ts",
    "lint-fix": "eslint src --ext .js --ext .ts --fix",
    "test": "cross-env jest --config __jest__/jest.config.js",
    "test-watch": "jest --watch --config __jest__/jest.config.js",
    "test-coverage": "jest --coverage --config __jest__/jest.config.js"
  },
  "dependencies": {
    "@types/react": "^19.1.12",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "babel-jest": "^30.1.2",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "favicons-webpack-plugin": "^6.0.1",
    "filemanager-webpack-plugin": "^9.0.1",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.4",
    "image-minimizer-webpack-plugin": "^4.1.4",
    "imagemin": "^9.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^8.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^11.0.1",
    "jest": "^30.1.2",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^30.1.2",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^10.3.1",
    "sass-loader": "^16.0.5",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "typescript-plugin-css-modules": "^5.2.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
